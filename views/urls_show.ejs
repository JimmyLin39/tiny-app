<!-- list full url by short form link -->
<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <!-- Add your EJS code here -->
  <header>
    <% include ./partials/_header %>
  </header>

  <main>
    <% for (let shortForm in urls){ %>
      <!-- match the respond id with urlDatabase -->
      <% if ( shortURL === shortForm) { %>
        <li><%= shortForm %>: <%= urls[shortForm] %></li>
      <% } %>
    <% } %>
  </main>

</body>
</html>